@charset "utf-8";
/* CSS Document */
body, ul, ol, li, p, h1, h2, h3, h4, h5, h6, form, fieldset, table, td, img, div, a, span {border:0 none;margin:0;padding:0;}
.clear{ clear:both;}
ul,li{ list-style-type:none;}
body a{ text-decoration:none;}
a:link, a:visited {color:#333; }
a:hover, a:active {color:#fc7f0f;}
.bank5{width:100%; height:5px; display:block; clear:both; font-size:0;}
.bank10{width:100%; height:10px; display:block; clear:both; font-size:0;}
.bank20{width:100%; height:20px; display:block; clear:both; font-size:0;}
.red{ color:#f00;}
.blue{color:#2A407C}
.yellow{ color:#FC7F0F;}
.green{ color:#5087a7 !important;}
.table_blue{ background:#d3edfc;}
body{ font-size:12px;background:#effbff;}
table { border-collapse: collapse; border-spacing: 0;}
font{ font-size:12px;}
.con_nowrap{font-family:arial;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
/*---------------------------------以上是公用代码--------------------------------*/
.header{ width:100%; height:85px; background:url(/images/header_bg.jpg) repeat-x;}
.header_logo{ width:100%; height:85px; background:url(/images/logo.jpg) no-repeat;}
.top_ri{ float:right; }
.top_ri .top_user{ padding-left:30px; line-height:20px; height:20px; color:#fff; float:right; margin:25px 20px 0 0; display:inline;}
.top_ri .top_user a:link,.top_ri .top_user a:visited {color:#f00; }
.top_ri .top_user a:hover,.top_ri .top_user a:active {color:#fc7f0f;}
.nav_menu{ float:right; }
.nav_menu ul{ background:url(/images/nav_menu_bg_le.jpg) no-repeat; float:right; padding-left:49px;}
.nav_menu ul li{ width:77px; height:32px; margin-right:2px; display:inline; float:left; text-align:center; background:url(/images/menu_li.jpg) no-repeat; font-weight:600; line-height:32px;}
.nav_menu ul li a:link,.nav_menu ul li a:visited { width:77px; height:32px; color:#515d75; display:block; overflow:hidden;}
.nav_menu ul li a:hover,.nav_menu ul li a:active {color:#515d75; background:url(/images/menu_hover.jpg) no-repeat; }
.nav_menu_hover{ width:70px; height:78px; color:#515d75; background:url(/images/menu_hover.jpg) no-repeat;}
.nav_index_ico,.nav_resourse_ico,.nav_content_ico,.nav_copyright_ico,.nav_number_ico,.nav_system_ico,.nav_out_ico{ width:32px; height:32px; background:url(/images/top_menu.png) no-repeat; _ background:url(/images/top_menu.gif) no-repeat; margin:15px 14px 0 ; display:inline-block;}
.nav_index_ico{ background-position:0 0;}
.nav_resourse_ico{ background-position:0 -40px;}
.nav_content_ico{ background-position:0 -80px;}
.nav_copyright_ico{ background-position:0 -120px;}
.nav_number_ico{ background-position:0 -160px;}
.nav_system_ico{ background-position:0 -200px;}
.nav_out_ico{ background-position:0 -240px; margin:15px 12px 0 16px;}
.nav_margin_ico{ width:32px; height:32px;  background:url(/images/component_preferences.png) no-repeat; _background:url(/images/component_preferences.gif) no-repeat; margin:15px 14px 0 ; display:inline-block;}
.nav_menu_text{ line-height:25px;}
/*---------------头部 end------------------*/
.ri_resources_ico,.resources_ico,.book_ico,.add_book_ico,.write_ico,.dele_book_ico,.pen_ico,.dele_ico,.arrow_ico,.ri_add_ico,.ri_modi_ico,.frozen_ico,.unlock_ico,.lock_ico,.indetail_ico,.toexamine_ico,.submit_ico,.export_ico,.link_author_ico,.link_product_ico{ background:url(/images/smart_ico.gif) no-repeat;}
.ri_resources_ico{ width:15px; height:13px; background-position:0 -100px; margin:7px 5px 0 10px;}
.resources_ico{ width:18px; height:19px; background-position:0 -60px; margin:4px 4px 0 10px ;}
.book_ico{width:3px; height:5px; background-position:0 0; margin:12px 10px 0 24px ;}
.add_book_ico{ width:14px; height:13px; background-position:0 -20px; margin:4px 5px 0 18px ;}
.frozen_ico{ width:16px; height:16px; background-position:0 -320px; margin:6px 5px 0 12px ;}/*冻结*/
.unlock_ico{ width:16px; height:16px; background-position:0 -380px; margin:6px 5px 0 12px ;}/*解锁*/
.lock_ico{ width:14px; height:15px; background-position:0 -360px; margin:6px 5px 0 12px ;}/*锁定*/
.indetail_ico{ width:16px; height:16px; background-position:0 -400px; margin:7px 5px 0 12px ;}/*查看详细信息*/
.submit_ico{ width:12px; height:12px; background-position:0 -240px; margin:8px 5px 0 12px ;}/*提交*/
.export_ico{ width:16px; height:16px; background-position:0 -200px; margin:7px 5px 0 12px ;}/*导出*/
.link_author_ico{ width:16px; height:16px; background-position:0 -160px; margin:5px 5px 0 12px ;}/*关联作家*/
.link_product_ico{ width:16px; height:16px; background-position:0 -80px; margin:7px 5px 0 12px ;}/*关联产品*/
.ri_add_ico{ width:14px; height:13px; background-position:0 -260px; margin:7px 4px 0 12px ;}/*增加*/
.ri_modi_ico{ width:14px; height:14px; background-position:0 -140px; margin:7px 4px 0 12px ;}/*修改*/
.dele_book_ico{ width:16px; height:16px; background-position:0 -180px; margin:7px 4px 0 12px ;}/*删除*/
.toexamine_ico{ width:16px; height:16px; background-position:0 -280px; margin:6px 4px 0 12px ;}/*审核*/
.write_ico{ width:14px; height:14px; background-position:0 -120px;}
.pen_ico{ width:11px; height:11px; background-position:0 -220px; display:inline-block; margin:0 4px 0 0}
.dele_ico{ width:10px; height:10px; background-position:0 -300px; display:inline-block; margin:0 4px 0 10px}
.arrow_ico{ width:10px; height:7px; background-position:0 -200px;}

