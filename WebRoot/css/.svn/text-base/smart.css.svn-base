.ri_role_sq_ico{ width:16px; height:16px; background:url(/images/246.gif) no-repeat; margin:7px 4px 0 10px ; display:inline-block;}
.excel_ico{ width:16px; height:16px; background:url(/images/excel_ico.jpg) no-repeat; margin:5px 5px 0 10px;}
.resource_up_ico{ width:14px; height:14px; background:url(/images/resource_up_ico.jpg) no-repeat; margin:5px 3px 0 10px;  display:inline-block;}
.resource_down_ico{ width:14px; height:14px; background:url(/images/resource_down_ico.jpg) no-repeat; margin:5px 3px 0 10px;  display:inline-block;}/*下载*/
