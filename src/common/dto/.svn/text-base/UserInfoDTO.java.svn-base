package cn.com.twsm.common.dto;

import cn.com.twsm.common.dto.BasicDTO;
/**
 * 用户个人信息DTO
 * @author QiaoGuangqing
 * @date 2012-04-06
 */
public class UserInfoDTO extends BasicDTO {
	private static final long serialVersionUID = 1L;
	
	public static final String SEX_MAN = "1";		//性别-男
	public static final String SEX_WOMAN = "2";		//性别-女
	public static final String SEX_UNKNOWN = "0";	//性别-未知
	
	private String sex = "0";				//性别[必填] 1:男 2:女 0:未知
	private String areaName;				//地区名称
	private String motto;					//座右铭
	private String rank = "0";				//等级[必填]
	private Integer readTimeAmount = 0;		//读书时间[必填]
	private Integer favoriteBookCount = 0;	//藏书数量[必填]
	
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getAreaName() {
		return areaName;
	}
	public void setAreaName(String areaName) {
		this.areaName = areaName;
	}
	public String getMotto() {
		return motto;
	}
	public void setMotto(String motto) {
		this.motto = motto;
	}
	public String getRank() {
		return rank;
	}
	public void setRank(String rank) {
		this.rank = rank;
	}
	public Integer getReadTimeAmount() {
		return readTimeAmount;
	}
	public void setReadTimeAmount(Integer readTimeAmount) {
		this.readTimeAmount = readTimeAmount;
	}
	public Integer getFavoriteBookCount() {
		return favoriteBookCount;
	}
	public void setFavoriteBookCount(Integer favoriteBookCount) {
		this.favoriteBookCount = favoriteBookCount;
	}

}
